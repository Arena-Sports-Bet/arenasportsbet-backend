generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String @id
  username      String 
  email         String @unique
  password      String
  birthdate     DateTime
  cpf           String @unique
  address       String
  zipcode       String
  country       String
  phone_number  String @unique
  state         String
  city          String

  @@map("users")
}